from(bucket: "ses_meo")
  |> range(start: -1)
  |> filter(fn: (r) => r["_measurement"] == "MCR.MONITORING")
  |> filter(fn: (r) => not contains(value: r.dataCenter, set: ["6", "7", "32", "39", "46", "49"]))
  |> filter(fn: (r) => r["_field"] == "reg_value")
  |> filter(fn: (r) => r["reg_id"] == "1584" or r["reg_id"] == "5584")
  |> keep(columns: ["_value","_time","reg_id", "gw_id", "chassis_id","slot_id", "mcr_id"])
//  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: true)
  |> yield()
